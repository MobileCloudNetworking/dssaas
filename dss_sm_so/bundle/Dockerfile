FROM mobilecloudnetworking/mcn_so_base_ops

RUN echo "" > /etc/resolv.conf
RUN echo "# Dynamic resolv.conf(5) file for glibc resolver(3) generated by resolvconf(8)" >> /etc/resolv.conf
RUN echo "#     DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN" >> /etc/resolv.conf
RUN echo "nameserver 10.2.2.2" >> /etc/resolv.conf
